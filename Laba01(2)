#include<stdio.h>
#include<math.h>

int main()
{
    int n, m, result;
    scanf("%d", &n);
    scanf("%d", &m);
    
    printf("\n\n");
    
    result = n+++m;
    printf(" your first result = %d\n", result);
    
    result = m-->n;
    printf(" your second result = %d\n", result);
    
    result = n-->m;
    printf(" your third result = %d\n", result);
    
    return 0;
}
