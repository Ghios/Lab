#include<stdio.h>
#include <cs50.h>

int main()
{
    int n, m, result;
    
    printf("Enter n : \n");
    n = GetInt();
    
    printf("Enter n : \n");
    m = GetInt();
    
    result = n+++m;
    printf("Your first result = %d\n", result);
    
    result = m-->n;
    printf("Your second result = %d\n", result);
    
    result = n-->m;
    printf("Your third result = %d\n", result);
    
    return 0;
}
